{
    "name"        : "jbzoo/data",
    "type"        : "library",
    "description" : "Extended implementation of ArrayObject",
    "license"     : "MIT",
    "keywords"    : ["ArrayObject", "Array", "Config", "Data", "ini", "yml", "params", "json"],
    "authors"     : [
        {
            "name"  : "Denis Smetannikov",
            "email" : "admin@jbzoo.com",
            "role"  : "lead"
        }
    ],
    "require"     : {
        "php"      : "^7.2",
        "ext-json" : "*"
    },

    "require-dev" : {
        "jbzoo/phpunit"             : "dev-master",
        "jbzoo/codestyle"           : "dev-master",
        "jbzoo/utils"               : "^3.1.2||^4.0",
        "jbzoo/profiler"            : "^1.0.5",
        "symfony/yaml"              : "^4.4||^5.0",
        "symfony/var-dumper"        : "^4.4||^5.0",
        "roave/security-advisories" : "dev-master"
    },

    "suggest"     : {
        "symfony/yaml" : ">=4.4",
        "jbzoo/utils"  : ">=3.0"
    },

    "autoload"    : {
        "files" : [
            "src/functions.php"
        ],
        "psr-4" : {
            "JBZoo\\Data\\" : "src"
        }
    }
}
