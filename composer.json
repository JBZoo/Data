{
    "name"              : "jbzoo/data",
    "type"              : "library",
    "description"       : "Extended implementation of ArrayObject",
    "license"           : "MIT",
    "keywords"          : ["ArrayObject", "Array", "Config", "Data", "ini", "yml", "params", "json"],
    "minimum-stability" : "stable",
    "authors"           : [
        {
            "name"  : "SmetDenis",
            "email" : "denis@jbzoo.com",
            "role"  : "lead"
        }
    ],
    "require"           : {
        "php"         : ">=7.1",
        "ext-json"    : "*",
        "jbzoo/utils" : "^2.3.3"
    },
    "require-dev"       : {
        "jbzoo/phpunit"               : "dev-master",
        "jbzoo/profiler"              : "^1.0.5",
        "symfony/yaml"                : "^4.4",
        "php-coveralls/php-coveralls" : "^2.2"
    },
    "suggest"           : {
        "symfony/yaml" : "^3.3.13"
    },
    "autoload"          : {
        "files" : [
            "src/functions.php"
        ],
        "psr-4" : {
            "JBZoo\\Data\\" : "src"
        }
    },
    "extra"             : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    },
    "config"            : {
        "optimize-autoloader" : true
    },
    "support"           : {
        "issues" : "https://github.com/SmetDenis/Data/issues"
    }
}
